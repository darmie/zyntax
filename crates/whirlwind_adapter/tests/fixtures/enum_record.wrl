module Test;

// Enum and Record test
enum Color {
    Red,
    Green,
    Blue,
    RGB(int, int, int)
}

model Point {
    var x: float,
    var y: float
}

function distance(p1: Point, p2: Point) -> float {
    var dx: float = p2.x - p1.x
    var dy: float = p2.y - p1.y
    return sqrt(dx * dx + dy * dy)
}
