module Test;

// Control flow test
function factorial(n: int) -> int {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

function sum(max: int) -> int {
    var total: int = 0
    var i: int = 0

    while i < max {
        total = total + i
        i = i + 1
    }

    return total
}
